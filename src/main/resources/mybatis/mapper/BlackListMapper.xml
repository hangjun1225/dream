<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hr.mapper.BlackListMapper">

	<select id="selectByPersonId" parameterType="java.lang.Integer" resultType="BlackList">
		SELECT * 
		FROM BlackList
		WHERE personId=#{personId}
	</select>

    
</mapper>